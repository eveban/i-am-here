{
  "expo": {
    "name": "I'm here",
    "description": "App para cadastro de localização de clientes",
    "slug":  "IAmHere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#243f4d"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.iamhere",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "package": "android.config.googleMaps.apiKey",
      "config": {
         "googleMaps": { "apiKey": "AIzaSyBmCWe3wRDMOT07OUJEXKUusMWbNEgcHaY" }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#243f4d"
      }
    }
  }
}
